import numpy as np
import matplotlib.pyplot as plt

#sklearn 라이브러리에서 기본 제공하는 손글씨 숫자 데이터셋
from sklearn.datasets import load_digits
#훈련 데이터, 테스트 데이터를 나눠주는 기능
